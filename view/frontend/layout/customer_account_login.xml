<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <block class="Deloitte\SocialLogin\Block\Popup\Social" before="customer.login.container" ifconfig="sociallogin/general/enabled" name="social-login-popup-authentication-social-login" template="Deloitte_SocialLogin::form/social.phtml">
                <block class="Deloitte\SocialLogin\Block\Popup" name="social-login-request-info" before="-" as="request.info" template="Deloitte_SocialLogin::popup/form/email.phtml"/>
            </block>
        </referenceContainer>
    </body>
</page>
